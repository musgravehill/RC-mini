function ChatbroLoader(b,c){var d={embedChatsParameters:b instanceof Array?b:[b],needLoadCode:typeof Chatbro==="undefined"},a=new XMLHttpRequest;a.withCredentials=true;a.onload=function(){eval(a.responseText)};a.onerror=function(){console.error("Chatbro loading error")};a.open("POST","//www.chatbro.com/embed_chats",c!==false);a.send(JSON.stringify(d))}
function ChatbroLoaderGet(b,c){var d={embedChatsParameters:b instanceof Array?b:[b],needLoadCode:typeof Chatbro==="undefined"},a=new XMLHttpRequest;a.withCredentials=true;a.onload=function(){eval(a.responseText)};a.onerror=function(){console.error("Chatbro loading error")};a.open("GET","//www.chatbro.com/embed_chats?parameters="+encodeURIComponent(JSON.stringify(d)),c!==false);a.send()}
function ChatbroLoaderPost(b,c){var d={embedChatsParameters:b instanceof Array?b:[b],needLoadCode:typeof Chatbro==="undefined"},a=new XMLHttpRequest;a.withCredentials=true;a.onload=function(){eval(a.responseText)};a.onerror=function(){console.error("Chatbro loading error")};a.open("POST","//www.chatbro.com/embed_chats",c!==false);a.setRequestHeader("Content-Type","application/x-www-form-urlencoded");a.send("parameters="+encodeURIComponent(JSON.stringify(d)))};
